.top-img-wrap {
    margin-top: 30rpx;
    width: 690rpx;
    height: 198rpx;
    position: relative;
    display: flex;
    justify-content: center;
}
.top-img-wrap image {
    width: 690rpx;
    height: 198rpx;
}
.top-img-wrap text {
    position: absolute;
    top: 80rpx;
}

