    align-items: center;
    margin-top: 24rpx;
}
.tips-wrap .pic-text-wrap image {
    width: 15rpx;
    height: 18rpx;
    margin-right: 12rpx;
}
.tips-wrap .you-can-do {
    line-height: 60rpx;
    margin-top: 24rpx;
}
